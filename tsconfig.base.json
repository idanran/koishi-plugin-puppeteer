{
  "compilerOptions": {
    "target": "es2022",
    "module": "esnext",
    "sourceMap": true,
    "declaration": true,
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "emitDeclarationOnly": true,
    "moduleResolution": "bundler",
    "strictBindCallApply": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@satorijs/element",
    "types": [
      "node",
      "yml-register/types",
    ],
  },
}
